{
    "extends": "airbnb",
    "parser": "babel-eslint",

    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "no-console": 0,
      "arrow-parens": 0,
      "padded-blocks": [0,1],
      "class-methods-use-this": 0,
      "arrow-body-style": [0, "always"],
      "react/no-typos": [0],
      "react/forbid-prop-types": [0],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],

      "object-curly-newline": 0,
      "function-paren-newline": 0
    }
}